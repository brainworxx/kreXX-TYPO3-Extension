{
  "name": "brainworxx/includekrexx",
  "type": "typo3-cms-extension",
  "description": "kreXX is a feature rich php debugger, featuring backend access to logfiles, code generation to reach the displayed values and much more. We added some special stuff for Fluid and Aimeos",
  "homepage": "https://github.com/brainworxx/kreXX-TYPO3-Extension",
  "license": [
    "LGPL-2.1-or-later"
  ],
  "keywords": [
    "debug",
    "vardump",
    "developer",
    "log"
  ],
  "require": {
    "typo3/cms-core": "^7.6 || ^8 || ^9 || ^10 || ^11",
    "php": "^7.0 || ^7.1 || ^7.2 || ^7.3 || ^7.4",
    "ext-json": "*"
  },
  "require-dev": {
    "typo3/cms-base-distribution": "8.7 || ^9.5 || ^10.4 || ^11.1",
    "phpunit/phpunit": "^6.5 || ^7.5 || ^8.5 || ^9.5",
    "php-mock/php-mock-phpunit": "^2",
    "aimeos/aimeos-typo3": "^18 || ^19 || ^20 || ^21"
  },
  "authors": [
    {
      "name": "Brainworxx",
      "homepage": "http://www.brainworxx.de/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Brainworxx\\Includekrexx\\": "Classes/",
      "Brainworxx\\Krexx\\": "Resources/Private/krexx/src/"
    },
    "files": [
      "Resources/Private/krexx/bootstrap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Brainworxx\\Krexx\\Tests\\": "Resources/Private/krexx/tests/",
      "Brainworxx\\Includekrexx\\Tests\\": "Tests/"
    },
    "files": [
      "Resources/Private/krexx/tests/Fixtures/Callback.php"
    ]
  },
  "replace": {
    "typo3-ter/includekrexx": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "includekrexx"
    }
  },
  "support": {
    "issues": "https://github.com/brainworxx/kreXX-TYPO3-Extension/issues"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --coverage-clover=./clover.xml --color -d memory_limit=-1 -c Build/phpunit.xml"
  }
}
