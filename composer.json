{
  "name": "brainworxx/includekrexx",
  "type": "typo3-cms-extension",
  "description": "kreXX is a feature rich php debugger with a neatly arranged debug window, featuring backend access to logfiles, code generation to reach the displayed values and much more.",
  "homepage": "https://github.com/brainworxx/kreXX-TYPO3-Extension",
  "license": ["LGPL-2.1-or-later"],
  "keywords": [
    "debug",
    "vardump",
    "developer",
    "log"
  ],
  "require": {
	"typo3/cms-core": ">=6.2.0 <=9.5.99",
	"php": ">=5.3.0 <=7.2.99"
  },
  "authors": [
    {
      "name": "Brainworxx",
      "homepage": "http://www.brainworxx.de/"
    }
  ],
  "autoload": {
        "psr-4": {
            "Brainworxx\\Includekrexx\\": "Classes/"
        }
    },
  "support": {
    "issues": "https://github.com/brainworxx/kreXX-TYPO3-Extension/issues"
  },
  "version": "3.0.0 dev"
}