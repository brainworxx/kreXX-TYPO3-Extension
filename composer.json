{
  "name": "brainworxx/includekrexx",
  "type": "typo3-cms-extension",
  "description": "kreXX is a feature rich php debugger with a neatly arranged debug window, featuring backend access to logfiles, code generation to reach the displayed values and much more.",
  "homepage": "https://github.com/brainworxx/kreXX-TYPO3-Extension",
  "license": [
    "LGPL-2.1-or-later"
  ],
  "keywords": [
    "debug",
    "vardump",
    "developer",
    "log"
  ],
  "require": {
    "typo3/cms-core": ">=7.6.0 <=10.3.99",
    "php": ">=5.5.0 <=7.4.99"
  },
  "require-dev": {
    "typo3/cms-base-distribution": "8.7 || ^9.5",
    "php-mock/php-mock-phpunit": "^2.4",
    "typo3/testing-framework": "^2.0 || ^3.8 || ^4.14",
    "aimeos/aimeos-typo3": "^18 || ^19"
  },
  "authors": [
    {
      "name": "Brainworxx",
      "homepage": "http://www.brainworxx.de/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Brainworxx\\Includekrexx\\": "Classes/",
      "Brainworxx\\Krexx\\": "Resources/Private/krexx/src/"
    },
    "files": [
      "Resources/Private/krexx/bootstrap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Brainworxx\\Krexx\\Tests\\": "Resources/Private/krexx/tests/",
      "Brainworxx\\Includekrexx\\Tests\\": "Tests/"
    }
  },
  "replace": {
    "typo3-ter/includekrexx": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "includekrexx"
    }
  },
  "support": {
    "issues": "https://github.com/brainworxx/kreXX-TYPO3-Extension/issues"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --color -d memory_limit=-1 -c Build/phpunit.xml"
  }
}