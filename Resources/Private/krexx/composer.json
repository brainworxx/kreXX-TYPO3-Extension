{
  "name": "brainworxx/krexx",
  "description": "kreXX is a php debugger, which displays debug information about objects and variables in it's own draggable debug output.",
  "keywords": [
    "debug",
    "debugging",
    "print_r",
    "var_dump"
  ],
  "type": "library",
  "homepage": "https://github.com/brainworxx/kreXX",
  "license": [
    "LGPL-2.1-or-later"
  ],
  "authors": [
    {
      "name": "Brainworxx",
      "homepage": "http://www.brainworxx.de/"
    }
  ],
  "require": {
    "php": ">=5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5"
  },
  "autoload": {
    "files": [
      "Krexx.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Brainworxx\\Krexx\\Tests\\": "tests/"
    }
  }
}